{
    "env": {
        "browser": true,
        "es6": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-restricted-syntax": "off",
        "no-constant-condition": "off",
        "radix": "off",
        "import/no-extraneous-dependencies": "off"
    }
}